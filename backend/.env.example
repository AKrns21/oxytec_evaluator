# Application Settings
APP_NAME=Oxytec Feasibility Platform
DEBUG=false
LOG_LEVEL=INFO

# Database
DATABASE_URL=postgresql+asyncpg://oxytec:oxytec_password@localhost:5432/oxytec_db
DB_ECHO=false
DB_POOL_SIZE=20

# Anthropic API
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-sonnet-4-5
ANTHROPIC_MODEL_HAIKU=claude-4-5-haiku-20250110

# OpenAI API (for embeddings and structured outputs)
OPENAI_API_KEY=your_openai_api_key_here
EMBEDDING_MODEL=text-embedding-3-small
EMBEDDING_DIMENSIONS=1536

# Agent-specific model configuration
# Note: GPT-5 models don't support temperature - it's mapped to reasoning_effort:
#   temp 0.0-0.3 → minimal, 0.4-0.7 → low, 0.8-1.0 → medium
EXTRACTOR_MODEL=gpt-5
EXTRACTOR_TEMPERATURE=0.2
PLANNER_MODEL=gpt-5-mini
PLANNER_TEMPERATURE=0.9
SUBAGENT_MODEL=gpt-5-nano
SUBAGENT_TEMPERATURE=0.4
RISK_ASSESSOR_MODEL=gpt-5
RISK_ASSESSOR_TEMPERATURE=0.4
WRITER_MODEL=claude-sonnet-4-5-20250929
WRITER_TEMPERATURE=0.4

# File Storage
UPLOAD_DIR=./uploads
MAX_UPLOAD_SIZE_MB=50

# Agent Configuration
MAX_SUBAGENTS=10
AGENT_TIMEOUT_SECONDS=300
MAX_TOOL_ITERATIONS=5

# CORS
CORS_ORIGINS=["http://localhost:3000","http://127.0.0.1:3000"]

# Redis (optional)
# REDIS_URL=redis://localhost:6379

# LangSmith Tracing (optional)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_ENDPOINT=https://eu.api.smith.langchain.com
LANGCHAIN_PROJECT=oxytec-feasibility-platform
